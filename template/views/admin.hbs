    <script>



        const port = window.location.port

        const getUrl = "https://www.koduapp-admin.com" + '/all'
        fetch(getUrl, { mode: "no-cors" }).then(res => {

            let sdata = res.json()

            return sdata


        }).then(data => {

            data.forEach(student => {
                let tab = document.getElementById('table1');

                let row = tab.insertRow(-1);

                let name = row.insertCell(0);

                let fathername = row.insertCell(1);
                let catergory = row.insertCell(2);
                let phone_number = row.insertCell(3);
                let address = row.insertCell(4);
                let campus = row.insertCell(5);
                let action = row.insertCell(6)

                name.classList.add('name')

                name.innerHTML = `<span>${student.name}</span`
                fathername.innerText = student.fathername
                catergory.innerText = student.category
                address.innerText = student.address
                phone_number.innerText = student.phone
                campus.innerText = student.campus
                action.innerHTML = `<a href="${window.location.origin}/student/?id=${student._id}"><div class="text-center"><i class="fa-solid fa-eye"></i></div></a>`
            })
        })


    </script>

    <script>
        const search = () => {
            const filterbox = document.getElementById('search').value.toUpperCase();
            const table = document.getElementById('table1');
            const tr = table.getElementsByTagName('tr');


            for (var i = 0; i <= tr.length; i++) {
                let td = tr[i].getElementsByTagName('td')[0];


                if (td) {
                    let textvalue = td.textContent || td.innerHTML;


                    if (textvalue.toUpperCase().indexOf(filterbox) > -1) {

                        tr[i].style.display = ""
                    }
                    else {
                        tr[i].style.display = "none"
                    }
                }
            }
        }
    </script>
    <script>
        const category_filter = () => {
            var e = document.getElementById("category-filter");
            var value = e.value.toUpperCase();
            var text = e.options[e.selectedIndex].text.toUpperCase();



            const table = document.getElementById('table1');
            const tr = table.getElementsByTagName('tr');
            for (var i = 0; i <= tr.length; i++) {
                let td = tr[i].getElementsByTagName('td')[2];


                if (td) {
                    let textvalue = td.textContent || td.innerHTML;


                    if (textvalue.toUpperCase().indexOf(text) > -1) {
                        console.log(text)
                        tr[i].style.display = ""
                    }
                    else if (text == "ALL") {
                        tr[i].style.display = ""
                    }
                    else {
                        tr[i].style.display = "none"
                        console.log(text)
                    }



                }
            }


        }       
    </script>
