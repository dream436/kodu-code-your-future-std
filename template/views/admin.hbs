<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kodu Admin-panel</title>

    <!-- bootstrap  -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- fontawsome -->
    <script src="https://kit.fontawesome.com/d937303721.js" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="style.css">


</head>

<body class="" style="overflow-x: hidden;">
    <header class="bg-white navbar align-content-center px-4 sticky-top">
        <div class="navbar-brand">
            <img style="width: 6rem;" src="https://koduapp.com/wp-content/uploads/2022/12/KodU-Logo-PNG12.png" alt=""
                srcset="">
        </div>
        <div>
            <ul class="nav nav-pills" id="pills-tab" role="tablist">
                <li class="nav-item" role="presentation">
                    <button class="nav-link active" id="pills-home-tab" data-bs-toggle="pill"
                        data-bs-target="#pills-home" type="button" role="tab" aria-controls="pills-home"
                        aria-selected="true">Lead</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="pills-profile-tab" data-bs-toggle="pill"
                        data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile"
                        aria-selected="false">Message</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="pills-contact-tab" data-bs-toggle="pill"
                        data-bs-target="#pills-contact" type="button" role="tab" aria-controls="pills-contact"
                        aria-selected="false">Fees</button>
                </li>
            </ul>
        </div>
        <div class="d-flex justify-content-end">

            <img src="data:image/jpeg;base64,"
                alt="" srcset="" class=" rounded-circle w-25">

        </div>
    </header>
    <main>
        <section class="pt-1">
            <div class="tab-content" id="pills-tabContent">
                <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab"
                    tabindex="0">
                    <div class="container">
                        <div
                            class="d-flex justify-content-between align-items-center align-content-lg-center py-3 px-3">
                            <h3>
                                Registered Students
                            </h3>
                            <div style="cursor: pointer;"><i class="fa-solid fa-download h5"></i></div>
                        </div>
                        <table class="table table-striped table-light">
                            <thead>
                                <tr class="text-capitalize">

                                    <th scope="col" class="text-capitalize">First Name</th>
                                    <th scope="col" class="text-capitalize">Last Name</th>
                                    <th scope="col" class="text-capitalize">Father Name</th>
                                    <th scope="col" class="text-capitalize">Mother Name</th>
                                    <th scope="col" class="text-capitalize">Phone Number</th>
                                    <th scope="col" class="text-capitalize">Address</th>
                                    <th scope="col" class="text-capitalize">Campus</th>
                                    <th scope="col">
                                        Action
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="table1">

                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="tab-pane fade" id="pills-profile" role="tabpanel" aria-labelledby="pills-profile-tab"
                    tabindex="0">PROFILE</div>
                <div class="tab-pane fade" id="pills-contact" role="tabpanel" aria-labelledby="pills-contact-tab"
                    tabindex="0">...</div>
            </div>
        </section>
    </main>


    <script>
 

        const port = window.location.port

        const getUrl = 'http://localhost:'+port+'/all'
 
        const idurl = 'http://localhost:'+port+'/all'
        fetch(getUrl).then(res => {
           
            let sdata = res.json()

            return sdata
            {{!-- return res.json(); --}}
          
        }).then(data => {
            console.log(data);

            data.forEach(student => {
                let tab = document.getElementById('table1');

                let row = tab.insertRow(-1);

                let name = row.insertCell(0);
                let lastname = row.insertCell(1);
                let fathername = row.insertCell(2);
                let mothername = row.insertCell(3);
                let phone_number = row.insertCell(4);
                let address = row.insertCell(5);
                let campus = row.insertCell(6);
                let action = row.insertCell(7)



                name.innerText = student.name
                lastname.innerText = student.lastname
                fathername.innerText = student.fathername
                mothername.innerText = student.mothername
                address.innerText = student.address
                phone_number.innerText = student.phone
                campus.innerText = student.campus
                action.innerHTML = `<a href="http://localhost:${port}/student/?id=${student._id}"><i class="fa-solid fa-eye"></i></a>`
            })
        })
    </script>
</body>

</html>